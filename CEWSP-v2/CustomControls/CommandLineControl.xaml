<UserControl x:Class="CEWSP_v2.CustomControls.CommandLineControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:CEWSP_v2.CustomControls"
             xmlns:p="clr-namespace:CEWSP_v2.Properties"
             xmlns:be="clr-namespace:CEWSP_Backend.Properties;assembly=CEWSP-Backend"
             mc:Ignorable="d">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Label Content="{x:Static p:MainWindowResources.LblCommandline}" />
            <TextBox x:Name="textBox" MinWidth="300" />
            <Button x:Name="btnLaunch" Content="{x:Static be:Resources.BtnLaunch}" Click="btnLaunch_Click" />
        </StackPanel>
    </Grid>
</UserControl>